#include <iostream>

using namespace std;

int main(void)
{
   int vetor[10];
   cin>>vetor[0];
   cout<<"X[0] = "<<vetor[0]<<endl;
   for(int i=0; i<10; i++)
   {
       cin
       if(vetor[i]<=0){
          vetor[i]=1;
       }
       else{
       cout<<"X["<<i<<"] = "<<vetor[i]<<endl;
       }
   }
}
